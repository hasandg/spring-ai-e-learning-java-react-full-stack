{
  "name": "e-learning-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "restart": "cd .. && ./restart.sh",
    "lint": "next lint",
    "lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "cypress open",
    "test:e2e:headless": "cypress run",
    "prepare": "husky install",
    "analyze": "ANALYZE=true next build",
    "build:dev": "env-cmd -f .env.development next build",
    "build:test": "env-cmd -f .env.test next build",
    "build:prod": "env-cmd -f .env.production next build",
    "start:dev": "env-cmd -f .env.development next start",
    "start:test": "env-cmd -f .env.test next start",
    "start:prod": "env-cmd -f .env.production next start",
    "clean": "rimraf .next out",
    "prebuild": "npm run clean"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
