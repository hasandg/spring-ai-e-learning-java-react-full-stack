{
  "name": "e-learning-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "restart": "cd .. && ./restart.sh",
    "lint": "next lint",
    "lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "analyze": "ANALYZE=true next build",
    "build:dev": "env-cmd -f .env.development next build",
    "build:test": "env-cmd -f .env.test next build",
    "build:prod": "env-cmd -f .env.production next build",
    "start:dev": "env-cmd -f .env.development next start",
    "start:test": "env-cmd -f .env.test next start",
    "start:prod": "env-cmd -f .env.production next start",
    "clean": "rimraf .next out",
    "prebuild": "npm run clean",
    "serve": "next dev"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.4",
    "@mui/icons-material": "^5.17.1",
    "@mui/material": "^5.17.1",
    "@mui/x-data-grid": "^6.20.4",
    "@mui/x-date-pickers": "^6.20.2",
    "@reduxjs/toolkit": "^2.2.1",
    "@stomp/stompjs": "^7.0.1",
    "@tanstack/react-query": "^5.69.0",
    "@types/node": "^20.17.24",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/redux-logger": "^3.0.13",
    "axios": "^1.6.8",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "env-cmd": "^10.1.0",
    "formik": "^2.4.5",
    "i18next": "^23.16.8",
    "keycloak-js": "^26.1.4",
    "match-sorter": "^8.0.0",
    "next": "^14.2.25",
    "notistack": "^3.0.2",
    "react": "^18.3.1",
    "react-custom-scrollbars-2": "^4.5.0",
    "react-dom": "^18.3.1",
    "react-i18next": "^14.1.3",
    "react-markdown": "^10.0.0",
    "react-player": "^2.15.1",
    "react-redux": "^9.1.0",
    "redux-logger": "^3.0.6",
    "swiper": "^11.0.7",
    "tailwind-merge": "^2.6.0",
    "usehooks-ts": "^3.1.1",
    "web-vitals": "^3.5.2",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.2.25",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/stompjs": "^2.3.9",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.25",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.9.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "rimraf": "^5.0.10",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{json,css,scss,md}": [
      "prettier --write"
    ]
  }
}
